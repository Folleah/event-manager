{
    "license": "MIT",
    "name": "folleah/psr7-event-emitter",
    "description": "PSR-7 event-emitter implementation",
    "keywords": ["psr7", "event"],
    "type": "library",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Andrey Volkov",
            "email": "folleah5@gmail.com",
            "homepage": "https://folleah.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "folleah/psr-interfaces": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
